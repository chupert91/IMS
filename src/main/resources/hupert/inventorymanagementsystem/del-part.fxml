<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane maxHeight="150" maxWidth="350" minHeight="150.0" minWidth="350.0" prefHeight="150.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hupert.inventorymanagementsystem.delPartController">
   <children>
      <Label layoutX="35.0" layoutY="36.0" text="Are you sure you want to delete this part?">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="confirmPartDel" layoutX="79.0" layoutY="83.0" mnemonicParsing="false" text="Delete" onAction="#onConfirmPartDel">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="cancelPartDel" layoutX="205.0" layoutY="83.0" mnemonicParsing="false" onAction="#onCancelClick" text="Cancel">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</Pane>
